<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>Navimate_home.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>login_verify</servlet-name>
        <servlet-class>com.login.login_verify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout_verify</servlet-name>
        <servlet-class>com.logout.logout_verify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>respond</servlet-name>
        <servlet-class>com.login.respond</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Team_Manage_Process</servlet-name>
        <servlet-class>com.nvmate.userServlets.Team_Manage_Process</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_verify</servlet-name>
        <url-pattern>/login_verify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout_verify</servlet-name>
        <url-pattern>/logout_verify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>respond</servlet-name>
        <url-pattern>/respond</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Team_Manage_Process</servlet-name>
        <url-pattern>/Team_Manage_Process</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
